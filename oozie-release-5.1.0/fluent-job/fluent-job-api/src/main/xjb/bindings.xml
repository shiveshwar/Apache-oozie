            https://raw.githubusercontent.com/Siggen/jaxb2-namespace-prefix/master/src/main/resources/prefix-namespace-schema.xsd">

    <jaxb:bindings schemaLocation = "../../../../../client/src/main/resources/distcp-action-1.0.xsd">

        <jaxb:schemaBindings>
